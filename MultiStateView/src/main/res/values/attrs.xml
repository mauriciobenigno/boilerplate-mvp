<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MultiStateView">
        <attr name="msv_loadingView" format="reference" />
        <attr name="msv_emptyView" format="reference" />
        <attr name="msv_errorView" format="reference" />
        <attr name="msv_errorNetworkView" format="reference" />
        <attr name="msv_errorUnknownView" format="reference" />
        <attr name="msv_noContentView" format="reference" />
        <attr name="msv_viewState" format="enum">
            <enum name="content" value="0" />
            <enum name="error" value="1" />
            <enum name="empty" value="2" />
            <enum name="loading" value="3" />
            <enum name="error_network" value="4" />
            <enum name="error_unknown" value="5" />
            <enum name="no_content" value="6" />
        </attr>
    </declare-styleable>

</resources>